{
  "extends": ["standard", "plugin:prettier/recommended"],
  "env": {
    "browser": false,
    "commonjs": true
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "rules": {
    "strict": "error",
    "import/order": [
      "error",
      {
        "newlines-between": "always"
      }
    ]
  }
}
